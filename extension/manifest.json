{
  "name": "go-get-v2-for-chrome",
  "description" : "fetch api of Bilibili, Iqiyi, Youku, Tencent",
  "version": "1.0",
  "permissions": [
    "tabs",
    "contextMenus",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "downloads"
  ],
  "options_page": "options.html",
  "icons": {
    "16": "hello_extensions.png"
  },
  "background": {
    "scripts": ["bundle.js"],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "hello_extensions.png",
    "default_popup": "main.html"
  },
  "manifest_version": 2
}
